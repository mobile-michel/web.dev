<header role="banner">
  <nav aria-label="Primary">
    <ul>
      <li><a href="/">Home</a></li>
      {%- for i in collections.primary %}
      <li><a href="{{i.url}}" {% if page.url==i.url %}aria-current="page" {% endif %}>{{i.data.title}}</a>
        <ul>
          {%- for item in collections.secondary -%}
          {%- if i.fileSlug == item.data.fileslug %}
          <li><a href="{{item.url}}" {% if page.url==item.url %}aria-current="page" {% endif %}>{{item.data.title}}</a></li>
          {%- endif %}
          {%- endfor %}
        </ul>
      </li>
      {%- endfor %}
    </ul>
  </nav>
  <form name="site-search" action="/search" method="get" role="search">
    <label for="query">Find the documentation</label>
    <input type="search" id="query" name="query" />
  </form>
</header>